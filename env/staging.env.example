# Staging Environment
# Copy to staging.env and fill in real values
NODE_ENV=staging
PORT=4000

# MongoDB - separate staging database
MONGO_DB_NAME=patta_staging
MONGO_USERNAME=your_staging_username
MONGO_PASSWORD=your_staging_password
MONGO_HOST=mongodb%2Bsrv://user:pass@your-cluster.mongodb.net/patta_staging

# Auth - generate with: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))"
JWT_SECRET=your_staging_jwt_secret

# CORS - staging frontend URL
CORS_ORIGINS=https://staging.yourdomain.com

# Sentry (optional)
# SENTRY_DSN=https://your-sentry-dsn
